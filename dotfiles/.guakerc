[general]
abbreviate-tab-names=false
compat-delete='delete-sequence'
display-n=0
display-tab-names=0
gtk-prefer-dark-theme=true
gtk-theme-name='Breeze-Dark'
gtk-use-system-default-theme=true
hide-tabs-if-one-tab=false
history-size=3000
load-guake-yml=true
max-tab-name-length=100
mouse-display=true
open-tab-cwd=true
prompt-on-quit=false
quick-open-command-line='code %(file_path)'
quick-open-enable=true
restore-tabs-notify=true
restore-tabs-startup=true
save-tabs-when-changed=true
schema-version='3.11.dev0'
scroll-keystroke=true
search-engine=1
start-at-login=true
start-fullscreen=false
startup-script=''
tab-ontop=true
use-default-font=true
use-popup=false
use-scrollbar=true
use-trayicon=false
window-halignment=2
window-height=54
window-horizontal-displacement=1
window-losefocus=true
window-ontop=true
window-refocus=false
window-tabbar=false
window-width=100

[keybindings/global]
show-hide='F1'

[keybindings/local]
clipboard-copy='<Primary>c'
clipboard-paste='<Primary>v'
close-tab='<Primary>w'
new-tab='<Primary>t'
search-terminal='<Primary>f'
split-tab-horizontal='<Primary><Shift>d'
switch-tab1='<Primary>1'
switch-tab2='<Primary>2'
switch-tab3='<Primary>3'
switch-tab4='<Primary>4'

[style]
cursor-shape=1

[style/background]
transparency=97

[style/font]
allow-bold=true
bold-is-bright=true
palette='#000000000000:#e5e522222222:#a6a6e3e32d2d:#fcfc95951e1e:#c4c48d8dffff:#fafa25257373:#6767d9d9f0f0:#f2f2f2f2f2f2:#555555555555:#ffff55555555:#5555ffff5555:#ffffffff5555:#55555555ffff:#ffff5555ffff:#5555ffffffff:#ffffffffffff:#ffffffffffff:#050505050505'
palette-name='Custom'
style='PragmataPro Mono Liga 11'
